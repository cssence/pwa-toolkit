doctype html
include mixins
html(lang="en")
	head
		meta(charset="utf-8")
		block title
			if title
				title= title
		meta(name="viewport", content="width=device-width,initial-scale=1.0")
		block styles
			+style("init.css")
		block icons
			link(rel="icon", sizes="16x16", type="image/gif", href="data:image/gif;base64,R0lGODlhEAAQAKECAAAAANNLbP///////yH5BAEKAAIALAAAAAAQABAAAAIglI9pIOwNmZqr0oulxDzp3gUiRCpiAJ7kBnZfG8UPCxYAOw==")
	body
		block content
		block scripts
